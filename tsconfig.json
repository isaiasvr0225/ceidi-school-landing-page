{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*" : ["src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@images/*" : ["./public/images/*"],
    }
  },
  "include": ["**/*.ts", "**/*.astro"]
}
